{
  "name": "quizGame",
  "version": "1.0.0",
  "author": "thelifeisyours <tliyours@gmail.com>",
  "license": "MIT",
  "main": "build/client/index.html",
  "scripts": {
    "start": "concurrently yarn:tsc:w yarn:server yarn:client",
    "build": "parcel build src/client/public/index.html",
    "tsc:w": "tsc -w",
    "server": "nodemon src/server --watch build/server",
    "client": "parcel watch src/client/public/index.html",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-react": "^7.13.13",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.37",
    "@types/pretty": "^2.0.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-router": "^5.1.12",
    "@types/react-router-dom": "^5.1.7",
    "concurrently": "^6.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "parcel": "^2.0.0-beta.2",
    "pretty": "^2.0.0",
    "supertest": "^6.1.3",
    "supertest-session": "^4.1.0",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "passport": "^0.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "recoil": "^0.1.3"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    },
    "presets": [
      "@babel/react"
    ]
  }
}
